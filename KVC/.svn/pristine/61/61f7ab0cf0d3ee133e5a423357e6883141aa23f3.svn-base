-(void)setNilValueForKey:(NSString *)key
{
    NSLog(@"setNilValueForKey:%@",key);
    if([key isEqualToString:@"hidden"])
        _hidden = NO;
}
-(BOOL)hidden
{
    NSLog(@"%s",__func__);
    return _hidden;
}
-(BOOL)isHidden
{
    NSLog(@"%s",__func__);
